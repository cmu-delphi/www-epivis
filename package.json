{
  "name": "www-epivis",
  "version": "2.1.5",
  "private": true,
  "license": "MIT",
  "description": "",
  "homepage": "https://github.com/cmu-delphi/www-epivis",
  "bugs": {
    "url": "https://github.com/cmu-delphi/www-epivis/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cmu-delphi/www-epivis.git"
  },
  "files": [
    "public/**.js",
    "public/**.map",
    "public/**.txt",
    "public/**.css",
    "!public/wrapper.*"
  ],
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "server": "sirv public --no-clear --port 8080",
    "validate": "svelte-check",
    "lint": "eslint src --ext .ts,.js,.svelte && prettier --check \"**/*\"",
    "fix": "eslint src --ext .ts,.js,.svelte --fix && prettier --write \"**/*\"",
    "prepare": "husky install",
    "prepack": "npm run build",
    "prettier": "prettier --write \"**/*\""
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/d3-array": "^2.11.0",
    "@types/d3-dsv": "^2.0.1",
    "@types/uikit": "^3.14.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "d3-array": "^2.12.1",
    "d3-dsv": "^2.0.0",
    "date-fns": "^2.29.2",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup": "^2.78.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.54.5",
    "shepherd.js": "^10.0.1",
    "sirv-cli": "^2.0.2",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.1",
    "svelte-fa": "^3.0.3",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "uikit": "^3.15.5"
  },
  "lint-staged": {
    "*.{ts,js,svelte}": "eslint --cache --fix",
    "*.{ts,js,css,md,svelte}": "prettier --write"
  }
}
