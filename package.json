{
  "name": "www-epivis",
  "version": "2.1.4",
  "private": true,
  "license": "MIT",
  "description": "",
  "homepage": "https://github.com/cmu-delphi/www-epivis",
  "bugs": {
    "url": "https://github.com/cmu-delphi/www-epivis/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cmu-delphi/www-epivis.git"
  },
  "files": [
    "public/**.js",
    "public/**.map",
    "public/**.txt",
    "public/**.css",
    "!public/wrapper.*"
  ],
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "server": "sirv public --no-clear --port 8080",
    "validate": "svelte-check",
    "lint": "eslint src --ext .ts,.js,.svelte && prettier --check \"**/*\"",
    "fix": "eslint src --ext .ts,.js,.svelte --fix && prettier --write \"**/*\"",
    "prepack": "npm run build",
    "prettier": "prettier --write \"**/*\""
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-typescript": "^12.1.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/d3-array": "^3.2.1",
    "@types/d3-dsv": "^3.0.7",
    "@types/uikit": "^3.14.5",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "d3-array": "^3.2.4",
    "d3-dsv": "^3.0.1",
    "date-fns": "^4.1.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-svelte": "^2.46.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "rollup": "^2.78.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-svelte": "^7.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.80.3",
    "shepherd.js": "^14.1.0",
    "sirv-cli": "^3.0.0",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.5",
    "svelte-fa": "^4.0.3",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "uikit": "^3.21.13"
  },
  "lint-staged": {
    "*.{ts,js,svelte}": "eslint --cache --fix",
    "*.{ts,js,css,md,svelte}": "prettier --write"
  }
}
