  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cmu-delphi/www-epivis.git"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
